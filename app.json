{
  "expo": {
    "name": "SpeedoDragster",
    "slug": "speedodragster",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/speedodragstericon.png",
    "scheme": [
      "speedodragster"
    ],
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/speedodragstericon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0049fe"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/speedodragstericon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.humood.speedodragster",
      "android": {
        "permissions": [
          "ACCESS_FINE_LOCATION"
        ]
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "594b51fa-3fa4-4e99-b4f0-4153bb39ec98"
      }
    },
    "owner": "humood"
  }
}
